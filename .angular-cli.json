{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "angular-presentation",
    "npmScope": "mycurrentapp",
    "latestMigration": "20180227-cleanup-scripts"
  },
  "apps": [
    {
      "root": "apps/mycurrentapp/src",
      "outDir": "dist/apps/mycurrentapp",
      "assets": [
        "../node_modules/intro.js",
        "assets",
        "favicon.ico",
        "service-worker.js"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "slides",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      },
      "name": "mycurrentapp"
    },
    {
      "name": "browser-window",
      "root": "libs/browser-window/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "differ",
      "root": "libs/differ/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "feedback",
      "root": "libs/feedback/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "ng2ts",
      "root": "libs/ng2ts/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "presentation",
      "root": "libs/presentation/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "tool-tips",
      "root": "libs/tool-tips/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "tracking",
      "root": "libs/tracking/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "utils",
      "root": "libs/utils/src",
      "test": "../../../test.js",
      "appRoot": ""
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "apps/mycurrentapp/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "mycurrentapp/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {},
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    }
  }
}
