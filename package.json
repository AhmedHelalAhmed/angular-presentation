{
  "name": "angular-presentation",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:ru": "ng serve --aot --i18n-file=src/i18n/messages.ru.xtb --locale=ru --i18n-format=xtb",
    "build:ru": "ng build --aot --i18n-file=src/i18n/messages.ru.xtb --locale=ru --i18n-format=xtb",
    "build": "ng build",
    "build:prod": "ng build --prod --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sw": "sw-precache --root=dist --config=sw-precache-config.js",
    "sw-serve": "ng build --prod --aot && npm run sw && cd dist && http-server --port=8080 --host=localhost --entry-file=/index.html",
    "deploy": "ng build --prod --aot && npm run sw && firebase deploy",
    "deploy:ru": "npm run build:ru && firebase deploy",
    "i18n": "ng xi18n -f=xmb --output-path src/i18n"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:nycJSorg/angular-presentation.git"
  },
  "public": true,
  "dependencies": {
    "angularfire2": "^5.0.0-rc.10",
    "core-js": "^2.4.1",
    "firebase": "^5.0.4",
    "hammerjs": "^2.0.8",
    "highlight.js": "^9.10.0",
    "intro.js": "^2.5.0",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "rxjs": "^6.2.0",
    "rxjs-compat": "^6.2.0",
    "vue": "^2.3.4",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.0-beta.1",
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.2.1",
    "@angular/cli": "^6.0.7",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/compiler-cli": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/platform-server": "^6.0.3",
    "@angular/router": "^6.0.3",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "babel-generator": "^6.25.0",
    "babel-traverse": "^6.25.0",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.3",
    "codelyzer": "^4.0.1",
    "eslint": "^4.2.0",
    "firebase-tools": "^3.18.6",
    "github-api": "^3.0.0",
    "gomoku-tools": "^0.1.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "plugin-typescript": "^7.0.6",
    "protractor": "^5.3.2",
    "sw-precache": "^5.2.0",
    "systemjs": "^0.20.12",
    "systemjs-builder": "^0.16.4",
    "ts-node": "~2.0.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.2",
    "zone.js": "^0.8.20"
  }
}
